{
  "name": "tailwind-suggest-canonical-classes",
  "version": "0.1.0",
  "description": "CLI to apply Tailwind CSS suggest_canonical_classes fixes",
  "type": "module",
  "bin": {
    "tailwind-suggest-canonical": "bin/tailwind-suggest-canonical.js"
  },
  "main": "dist/cli.js",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "tailwindcss",
    "lint",
    "formatter",
    "cli",
    "suggest-canonical-classes"
  ],
  "license": "MIT",
  "dependencies": {
    "@laststance/tailwindcss-canonical-core": "workspace:*",
    "minimist": "^1.2.8",
    "tinyglobby": "^0.2.12"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.5",
    "@types/node": "^20.17.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  }
}
