{
  "name": "@laststance/tailwindcss-canonical-core",
  "version": "0.1.0",
  "description": "Core logic for Tailwind CSS canonical class transformation",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc -p tsconfig.json --noEmit"
  },
  "keywords": [
    "tailwindcss",
    "tailwind",
    "canonical",
    "suggest-canonical-classes",
    "css"
  ],
  "author": "Laststance.io",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laststance/tailwindcss-canonical-classes-monrepo.git",
    "directory": "packages/core"
  },
  "homepage": "https://github.com/laststance/tailwindcss-canonical-classes-monrepo/tree/main/packages/core#readme",
  "bugs": {
    "url": "https://github.com/laststance/tailwindcss-canonical-classes-monrepo/issues"
  },
  "engines": {
    "node": ">=18"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@tailwindcss/language-service": "0.14.29",
    "jiti": "^2.6.0",
    "vscode-languageserver-textdocument": "1.0.12"
  },
  "devDependencies": {
    "@types/node": "^20.17.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "tailwindcss": "^4.0.0"
  }
